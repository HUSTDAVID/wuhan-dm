<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical" >
    
    <View
        android:id="@+id/mzine_img_header_line1"
        android:layout_width="fill_parent"
        android:layout_height="2dip"
        android:background="@drawable/double_line_hor"/>

    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <ImageView
            android:id="@+id/mzine_img_header_imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="314"
            android:src="@drawable/temp_dm_4x_1" />
    
    <View
            android:id="@+id/mzine_img_header_line2"
            android:layout_width="2dip"
            android:layout_height="199.33dip"
            android:layout_weight="3"
            android:background="@color/mzine_img" />
    
    <LinearLayout 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="163"
        android:orientation="vertical">
        
        <ImageView
        android:id="@+id/mzine_img_header_imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="149"
        android:src="@drawable/temp_dm_1x_2" />

        <View
        android:id="@+id/mzine_img_header_line3"
        android:layout_width="110dip"
        android:layout_height="0.67dip"
        android:layout_weight="1"
        android:background="@color/mzine_img"/>
        
    <ImageView
        android:id="@+id/mzine_img_header_imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="149"
        android:src="@drawable/temp_dm_1x_1" />
        
    </LinearLayout>

    

</LinearLayout>

</LinearLayout>